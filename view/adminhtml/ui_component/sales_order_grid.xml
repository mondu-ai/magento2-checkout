<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
<listingToolbar name="listing_top">
    <massaction name="listing_massaction">
        <action name="mass_custom_action">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="type" xsi:type="string">mass_custom_action</item>
                    <item name="label" xsi:type="string" translate="true">Mondu: Invoice orders</item>
                    <item name="url" xsi:type="url" path="mondu/bulk/ship"/>
                </item>
            </argument>
        </action>
        <action name="mass_order_sync_mondu">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="type" xsi:type="string">mass_order_sync_mondu</item>
                    <item name="label" xsi:type="string" translate="true">Mondu: Sync orders</item>
                    <item name="url" xsi:type="url" path="mondu/bulk/sync"/>
                </item>
            </argument>
        </action>
    </massaction>
</listingToolbar>
</listing>
